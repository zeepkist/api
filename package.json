{
  "name": "@zeepkist/gtr-api",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Fully typed API client for the Zeepkist GTR API",
  "author": "James Harris <wopian@wopian.me>",
  "homepage": "https://github.com/zeepkist/gtr-api#readme",
  "repository": "https://github.com/zeepkist/gtr-api",
  "bugs": {
    "url": "https://github.com/zeepkist/gtr-api/issues"
  },
  "funding": "https://github.com/sponsors/wopian",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore",
    "lint:ci": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts --ignore-path .gitignore",
    "test": "ava",
    "coverage": "c8 ava",
    "document": "typedoc"
  },
  "devDependencies": {
    "@ava/typescript": "~4.0.0",
    "@rushstack/eslint-patch": "~1.2.0",
    "@semantic-release/changelog": "~6.0.2",
    "@semantic-release/commit-analyzer": "~9.0.2",
    "@semantic-release/github": "~8.0.7",
    "@semantic-release/npm": "~9.0.2",
    "@semantic-release/release-notes-generator": "~10.0.3",
    "@types/node": "~18.15.0",
    "@typescript-eslint/eslint-plugin": "~5.54.0",
    "@typescript-eslint/parser": "~5.54.0",
    "ava": "~5.2.0",
    "c8": "~7.13.0",
    "eslint": "~8.35.0",
    "eslint-config-prettier": "~8.7.0",
    "eslint-plugin-import": "~2.27.4",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-simple-import-sort": "~10.0.0",
    "eslint-plugin-unicorn": "~46.0.0",
    "prettier": "~2.8.3",
    "semantic-release": "~20.1.0",
    "tsx": "~3.12.2",
    "typedoc": "~0.23.25",
    "typescript": "~4.9.4"
  },
  "dependenciesMeta": {
    "esbuild": {
      "built": true
    }
  },
  "ava": {
    "utilizeParallelBuilds": true,
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader",
      "tsx"
    ]
  },
  "c8": {
    "all": true,
    "skipFull": true,
    "src": [
      "dist"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      ".pnp.cjs",
      ".pnp.loader.mjs",
      "dist/**/*.d.ts",
      "dist/**/*.spec.js"
    ]
  },
  "renovate": {
    "extends": [
      "local>wopian/renovate-config"
    ]
  },
  "packageManager": "yarn@3.4.1",
  "dependencies": {
    "ky": "~0.33.2",
    "ky-universal": "~0.11.0"
  },
  "keywords": [
    "gtr",
    "zeepkist",
    "api",
    "client",
    "typescript"
  ]
}
