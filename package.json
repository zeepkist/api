{
  "name": "@zeepkist/gtr-api",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Fully typed API client for the Zeepkist GTR API",
  "author": "James Harris <wopian@wopian.me>",
  "homepage": "https://github.com/zeepkist/gtr-client#readme",
  "repository": "https://github.com/zeepkist/gtr-client",
  "bugs": {
    "url": "https://github.com/zeepkist/gtr-client/issues"
  },
  "funding": "https://github.com/sponsors/wopian",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore",
    "lint:ci": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts --ignore-path .gitignore",
    "test": "ava",
    "coverage": "c8 ava",
    "document": "typedoc"
  },
  "dependencies": {
    "ky": "~1.0.1",
    "pako": "~2.1.0"
  },
  "devDependencies": {
    "@ava/typescript": "~4.1.0",
    "@rushstack/eslint-patch": "~1.5.0",
    "@semantic-release/changelog": "~6.0.2",
    "@semantic-release/commit-analyzer": "~11.0.0",
    "@semantic-release/github": "~9.2.0",
    "@semantic-release/npm": "~11.0.0",
    "@semantic-release/release-notes-generator": "~12.0.0",
    "@types/node": "~20.8.0",
    "@types/pako": "~2.0.0",
    "@typescript-eslint/eslint-plugin": "~6.7.0",
    "@typescript-eslint/parser": "~6.7.0",
    "ava": "~5.3.0",
    "c8": "~8.0.0",
    "eslint": "~8.50.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-import": "~2.28.0",
    "eslint-plugin-prettier": "~5.0.0",
    "eslint-plugin-simple-import-sort": "~10.0.0",
    "eslint-plugin-unicorn": "~48.0.0",
    "prettier": "~3.0.0",
    "semantic-release": "~22.0.0",
    "tsx": "~3.13.0",
    "typedoc": "~0.25.0",
    "typescript": "~5.2.0"
  },
  "dependenciesMeta": {
    "esbuild": {
      "built": true
    }
  },
  "ava": {
    "utilizeParallelBuilds": true,
    "typescript": {
      "compile": "tsc",
      "rewritePaths": {
        "src/": "dist/src/"
      }
    }
  },
  "c8": {
    "all": true,
    "skipFull": true,
    "src": [
      "dist"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      ".pnp.cjs",
      ".pnp.loader.mjs",
      "dist/**/*.d.ts",
      "dist/**/*.spec.js"
    ]
  },
  "renovate": {
    "extends": [
      "local>wopian/renovate-config"
    ]
  },
  "packageManager": "yarn@3.6.3",
  "keywords": [
    "gtr",
    "zeepkist",
    "api",
    "client",
    "typescript"
  ]
}
